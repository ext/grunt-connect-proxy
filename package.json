{
  "name": "grunt-connect-proxy3",
  "description": "Provides a http proxy as middleware for grunt connect.",
  "version": "2.0.0",
  "homepage": "https://github.com/ext/grunt-connect-proxy",
  "author": {
    "name": "Drewz",
    "email": "drewz@2mod5.com"
  },
  "contributors": [
    "David Sveningsson <ext-github@sidvind.com>",
    "Bj√∂rn Harrtell <bjorn@wololo.org>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ext/grunt-connect-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/ext/grunt-connect-proxy/issues"
  },
  "license": "MIT",
  "main": "Gruntfile.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "prettier:check": "prettier '**/*.{ts,js,json,md,scss,vue}' --list-different",
    "prettier:write": "prettier '**/*.{ts,js,json,md,scss,vue}' --write",
    "test": "grunt eslint test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ],
    "automerge": true,
    "automergeType": "branch",
    "prCreation": "not-pending",
    "unpublishSafe": true,
    "major": {
      "automerge": false
    }
  },
  "dependencies": {
    "grunt": "1.0.1",
    "http-proxy": "1.15.2",
    "lodash": "4.16.4"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-sidvind": "^1.3.2",
    "eslint-plugin-prettier": "^3.1.0",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-connect": "1.0.2",
    "grunt-contrib-nodeunit": "1.0.0",
    "grunt-eslint": "^21.0.0",
    "husky": "^2.3.0",
    "is-ci": "^2.0.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "peerDependencies": {
    "grunt": "^1.0.1"
  },
  "keywords": [
    "gruntplugin",
    "proxy",
    "connect",
    "http",
    "grunt"
  ]
}
